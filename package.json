{
  "name": "game-engine",
  "version": "0.1.0",
  "scripts": {
    "start": "node devServer.js",
    "build": "rimraf ./build && NODE_ENV=production webpack --config webpack.prod.config.js",
    "build:dll": "webpack --config ./webpack.dll.config.js",
    "postinstall": "npm run build:dll",
    "precommit": "lint-staged",
    "format": "prettier --single-quote --write \"src/**/*.js\" \"*.js\"",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src- --fix",
    "copy-to-root": "cp -a ./build/. ."
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "eslint --cache --ignore-pattern .gitignore --color --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "axios": "^0.18.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "^0.11.1",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "postcss-loader": "^1.3.3",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-color": "^2.14.0",
    "react-dom": "^15.4.2",
    "react-flex-material": "^1.0.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "rheostat": "^2.1.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "styled-components": "^1.4.5",
    "tokeys": "^1.1.0",
    "url-loader": "^0.5.8",
    "whatwg-fetch": "^2.0.3",
    "xmlhttprequest": "1.8.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "babel-plugin-react-transform": "^2.0.2",
    "compression": "^1.6.2",
    "connect-modrewrite": "^0.10.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.3",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-transform-hmr": "1.0.4",
    "rimraf": "^2.6.1",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "2.15.0"
  }
